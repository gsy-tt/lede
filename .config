# 基础系统配置
CONFIG_TARGET_qualcommax=y
CONFIG_TARGET_qualcommax_IPQ53XX=y
CONFIG_TARGET_qualcommax_DEVICE_qcom_ipq5332_jdcloud_be6500=y

# 选择 IPQ5332 SOC
CONFIG_TARGET_BOARD="qualcommax"
CONFIG_TARGET_BOARD_PART="ipq53xx"
CONFIG_TARGET_PROFILE="qcom_ipq5332_jdcloud_be6500"

# 内核模块 - 网络设备
CONFIG_PACKAGE_kmod-qca8386=y
CONFIG_PACKAGE_kmod-usb3=y
CONFIG_PACKAGE_kmod-usb-core=y
CONFIG_PACKAGE_kmod-usb-ohci=y
CONFIG_PACKAGE_kmod-usb2=y

# 内核模块 - 无线驱动
CONFIG_PACKAGE_kmod-ath11k=y
CONFIG_PACKAGE_ath11k-firmware-qcn6224=y

# 文件系统支持
CONFIG_PACKAGE_kmod-fs-ext4=y
CONFIG_PACKAGE_e2fsprogs=y
CONFIG_PACKAGE_kmod-fs-f2fs=y  # 可选，支持 F2FS 文件系统
CONFIG_PACKAGE_kmod-fs-exfat=y  # 可选，支持 exFAT

# 实用工具
CONFIG_PACKAGE_parted=y
CONFIG_PACKAGE_fdisk=y
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_iotop=y

# LuCI 界面
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-theme-argon=y  # 可选，美观主题
CONFIG_PACKAGE_luci-app-firewall=y
CONFIG_PACKAGE_luci-app-nlbwmon=y  # 带宽监控
CONFIG_PACKAGE_luci-app-sqm=y  # QoS 支持

# 存储和文件系统
CONFIG_BLK_DEVICES=y
CONFIG_PARTITION_TABLES=y
CONFIG_DMSQUASHFS=y
CONFIG_SQUASHFS=y

# 调试工具
CONFIG_PACKAGE_kmod-debug-core=y
CONFIG_PACKAGE_kmod-debugfs=y
CONFIG_PACKAGE_kmod-usbcore-debug=y
